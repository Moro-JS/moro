{
  "name": "@morojs/moro",
  "version": "1.6.3",
  "description": "High-performance Node.js framework with intelligent routing, automatic middleware ordering, enterprise authentication (Auth.js), type-safe validation, and functional architecture",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepare": "husky",
    "prepublish": "npm run build",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:unit": "jest --testPathPatterns=unit --detectOpenHandles",
    "test:integration": "jest --testPathPatterns=integration --detectOpenHandles",
    "test:e2e": "jest --testPathPatterns=e2e --detectOpenHandles --testTimeout=30000",
    "test:debug": "jest --detectOpenHandles --verbose",
    "test:ci": "jest --coverage --watchAll=false --ci --forceExit",
    "test:force": "jest --forceExit",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:dist": "npm run build && node scripts/validate-dist.mjs",
    "test:package": "npm run build && node scripts/validate-package.mjs",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint-staged": "lint-staged",
    "docs:build": "typedoc --out docs-html src/index.ts",
    "docs:serve": "http-server docs-html -p 8080",
    "docs:json": "typedoc --json docs-json/api.json src/index.ts",
    "docs:markdown": "typedoc --plugin typedoc-plugin-markdown --out docs-md src/index.ts",
    "docs:all": "npm run docs:build && npm run docs:json && npm run docs:markdown",
    "prerelease": "npm run build && npm test && npm run test:package && node scripts/release.js",
    "prerelease:patch": "npm run build && npm test && npm run test:package && node scripts/release.js patch",
    "prerelease:minor": "npm run build && npm test && npm run test:package && node scripts/release.js minor",
    "prerelease:major": "npm run build && npm test && npm run test:package && node scripts/release.js major"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "framework",
    "http-server",
    "websockets",
    "intelligent-routing",
    "circuit-breaker",
    "rate-limiting",
    "dependency-injection",
    "mysql",
    "enterprise",
    "high-performance",
    "native",
    "functional",
    "zod",
    "validation",
    "moro",
    "api"
  ],
  "author": "Moro Framework Team",
  "license": "MIT",
  "homepage": "https://morojs.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Moro-JS/moro.git"
  },
  "bugs": {
    "url": "https://github.com/Moro-JS/moro/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@auth/core": "^0.37.3",
    "better-sqlite3": "^12.2.0",
    "class-validator": "^0.14.0",
    "drizzle-orm": "^0.44.5",
    "ioredis": "^5.7.0",
    "joi": "^17.0.0",
    "mongodb": "^6.19.0",
    "mysql2": "^3.14.5",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "swagger-ui-dist": "^5.28.1",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.52.0",
    "ws": "^8.18.0",
    "yup": "^1.7.1"
  },
  "peerDependenciesMeta": {
    "@auth/core": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "swagger-ui-dist": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "drizzle-orm": {
      "optional": true
    },
    "socket.io": {
      "optional": true
    },
    "uWebSockets.js": {
      "optional": true
    },
    "ws": {
      "optional": true
    },
    "zod": {
      "optional": true
    },
    "joi": {
      "optional": true
    },
    "yup": {
      "optional": true
    },
    "class-validator": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@jest/globals": "^30.1.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "depcheck": "^1.4.7",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.1.6",
    "prettier": "^3.4.2",
    "socket.io": "^4.8.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.7.0",
    "typedoc": "^0.28.12",
    "typedoc-github-theme": "^0.3.1",
    "typedoc-material-theme": "^1.4.0",
    "typedoc-plugin-extras": "^4.0.1",
    "typedoc-plugin-markdown": "^4.8.1",
    "typedoc-theme-hierarchy": "^6.0.0",
    "typescript": "^5.9.2",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.52.0",
    "ws": "^8.18.3",
    "zod": "^4.1.8"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{json,md}": [
      "prettier --write"
    ]
  }
}
