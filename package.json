{
  "name": "@morojs/moro",
  "version": "1.7.10",
  "description": "High-performance Node.js framework with intelligent routing, automatic middleware ordering, enterprise authentication (Auth.js), type-safe validation, and functional architecture",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepare": "husky",
    "prepublish": "npm run build",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:unit": "jest --testPathPatterns=unit --detectOpenHandles",
    "test:integration": "jest --testPathPatterns=integration --detectOpenHandles",
    "test:e2e": "jest --testPathPatterns=e2e --detectOpenHandles --testTimeout=30000",
    "test:debug": "jest --detectOpenHandles --verbose",
    "test:ci": "jest --coverage --watchAll=false --ci --forceExit",
    "test:force": "jest --forceExit",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:dist": "npm run build && node scripts/validate-dist.mjs",
    "test:package": "npm run build && node scripts/validate-package.mjs",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint-staged": "lint-staged",
    "docs:build": "typedoc --out docs-html src/index.ts",
    "docs:serve": "http-server docs-html -p 8080",
    "docs:json": "typedoc --json docs-json/api.json src/index.ts",
    "docs:markdown": "typedoc --plugin typedoc-plugin-markdown --out docs-md src/index.ts",
    "docs:all": "npm run docs:build && npm run docs:json && npm run docs:markdown",
    "check:uws": "node scripts/check-uws-version.mjs",
    "update:uws": "node scripts/check-uws-version.mjs --update",
    "prerelease": "npm run lint:fix && npm run format && npm run build && node scripts/release.js",
    "prerelease:patch": "npm run lint:fix && npm run format && npm run build && node scripts/release.js patch",
    "prerelease:minor": "npm run lint:fix && npm run format && npm run build && node scripts/release.js minor",
    "prerelease:major": "npm run lint:fix && npm run format && npm run build && node scripts/release.js major"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "framework",
    "http-server",
    "websockets",
    "intelligent-routing",
    "circuit-breaker",
    "rate-limiting",
    "dependency-injection",
    "enterprise",
    "high-performance",
    "native",
    "functional",
    "moro",
