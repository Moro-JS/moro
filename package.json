{
  "name": "@morojs/moro",
  "version": "1.1.0",
  "description": "High-performance Node.js framework with intelligent routing, automatic middleware ordering, type-safe Zod validation, and functional architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "dev:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepare": "husky",
    "prepublish": "npm run build",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:unit": "jest --testPathPatterns=unit --detectOpenHandles",
    "test:integration": "jest --testPathPatterns=integration --detectOpenHandles",
    "test:e2e": "jest --testPathPatterns=e2e --detectOpenHandles --testTimeout=30000",
    "test:debug": "jest --detectOpenHandles --verbose",
    "test:ci": "jest --coverage --watchAll=false --ci --forceExit",
    "test:force": "jest --forceExit",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint-staged": "lint-staged",
    "docs:build": "typedoc --out docs src/index.ts",
    "docs:serve": "http-server docs -p 8080"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "framework",
    "http-server",
    "websockets",
    "intelligent-routing",
    "circuit-breaker",
    "rate-limiting",
    "dependency-injection",
    "mysql",
    "enterprise",
    "high-performance",
    "native",
    "functional",
    "zod",
    "validation",
    "moro",
    "api"
  ],
  "author": "Moro Framework Team",
  "license": "MIT",
  "homepage": "https://morojs.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Moro-JS/moro.git"
  },
  "bugs": {
    "url": "https://github.com/Moro-JS/moro/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "socket.io": "^4.8.1",
    "zod": "^4.1.5"
  },
  "peerDependencies": {
    "better-sqlite3": "^12.2.0",
    "drizzle-orm": "^0.44.5",
    "ioredis": "^5.7.0",
    "mongodb": "^6.19.0",
    "mysql2": "^3.14.5",
    "pg": "^8.16.3",
    "swagger-ui-dist": "^5.28.1",
    "ts-node": "^10.9.1"
  },
  "peerDependenciesMeta": {
    "mysql2": {
      "optional": true
    },
    "swagger-ui-dist": {
      "optional": true
    },
    "ts-node": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "drizzle-orm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@jest/globals": "^30.1.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "depcheck": "^1.4.7",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.1.6",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE",
    "API.md",
    "tsconfig.json"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/tests/**/*.test.ts",
      "**/src/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {}
      ]
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts",
      "!src/**/index.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "testTimeout": 15000,
    "maxWorkers": 1,
    "forceExit": false,
    "detectOpenHandles": true,
    "openHandlesTimeout": 1000,
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{json,md}": [
      "prettier --write"
    ]
  }
}
